<UserControl x:Class="SchedulingClients.UserControls.AutoServicerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SchedulingClients.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <local:LabelContentControl Header="Auto Servicing Enabled">
            <CheckBox IsEnabled="true" IsChecked="{Binding Path=IsEnabled}" />
        </local:LabelContentControl>
    </Grid>
</UserControl>